<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secound_primary"
    tools:context=".screen.fragment.items.EveningSaysFragment">

    <TextView
        android:id="@+id/evening_toolbar"
        style="@style/ToolbarApplicationStyle"
        android:text="أذكار المساء"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icon_back"
        style="@style/IconToolbarStyle"
        app:layout_constraintBottom_toBottomOf="@id/evening_toolbar"
        app:layout_constraintStart_toStartOf="@id/evening_toolbar"
        app:layout_constraintTop_toTopOf="@id/evening_toolbar" />

    <TextView
        android:id="@+id/tv_evening"
        style="@style/TextDuaStyle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/evening_toolbar"
        tools:text="@string/test_text" />

</androidx.constraintlayout.widget.ConstraintLayout>